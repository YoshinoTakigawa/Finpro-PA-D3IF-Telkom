<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="content_all_text_bold_color_primary">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:layout_marginBottom">@dimen/dimen_medium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">@dimen/text_size_title_small</item>
    </style>

    <style name="content_all_text_bold_color_black">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:textColor">@color/colorTextBlack</item>
        <item name="android:textSize">@dimen/text_size_title_small</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="content_all_text_color_black">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:textColor">@color/colorTextBlack</item>
        <item name="android:textSize">@dimen/text_size_title_small</item>
    </style>

    <style name="content_all_list_title">
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_default</item>
        <item name="android:textColor">@color/colorTextBlack</item>
    </style>

    <style name="content_all_list_sub_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textStyle">bold</item>
        <item name="android:text">@string/dummy</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="all_image_informasi">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">@dimen/dimen_profil_photo_list</item>
        <item name="android:layout_height">@dimen/dimen_profil_photo_list</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
    </style>




</resources>