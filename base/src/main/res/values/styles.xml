<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="content_all_text_bold_color_primary">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:layout_marginBottom">@dimen/dimen_medium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">@dimen/text_size_title_small</item>
    </style>

    <style name="content_all_text_bold_color_black">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:textColor">@color/colorTextBlack</item>
        <item name="android:textSize">@dimen/text_size_title_small</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="content_all_text_color_black">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:textColor">@color/colorTextBlack</item>
        <item name="android:textSize">@dimen/text_size_title_small</item>
    </style>

    <style name="content_all_list_title">
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_default</item>
        <item name="android:textColor">@color/colorTextBlack</item>
    </style>

    <style name="content_all_list_sub_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textStyle">bold</item>
        <item name="android:text">@string/dummy</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="all_image_informasi">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">@dimen/dimen_profil_photo_list</item>
        <item name="android:layout_height">@dimen/dimen_profil_photo_list</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
    </style>

    <style name="view_all_line_horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginTop">@dimen/dimen_medium</item>
        <item name="android:layout_marginBottom">@dimen/dimen_medium</item>
        <item name="android:background">@color/colorBackground</item>
    </style>

    <style name="cardview_list_all_clicked">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/dimen_medium</item>
        <item name="android:layout_marginStart">@dimen/dimen_medium</item>
        <item name="android:layout_marginEnd">@dimen/dimen_medium</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
    </style>

    <style name="spinner_all">
        <item name="android:singleLine">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/dummt_spinner</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:padding">@dimen/dimen_medium</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="fab_all_add">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_margin">@dimen/dimen_big</item>
        <item name="android:src">@drawable/ic_fab_add</item>
    </style>

    <style name="image_all_editor_view">
        <item name="android:layout_width">@dimen/dimen_profil_photo</item>
        <item name="android:layout_height">@dimen/dimen_profil_photo</item>
        <item name="android:layout_marginBottom">@dimen/dimen_big_ultra</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:contentDescription">@string/dummy</item>
    </style>

    <style name="textview_title_all_editor_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dimen_small</item>
        <item name="android:layout_marginTop">@dimen/dimen_medium</item>
        <item name="android:layout_marginBottom">@dimen/dimen_medium</item>
        <item name="android:textSize">@dimen/text_size_title_small</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="button_all_primary">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorTextWhite</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
        <item name="android:background">@drawable/box_border_button_primary</item>
    </style>

    <style name="profile_all_textview_content">
        <item name="android:layout_width">@dimen/dimen_weight</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginStart">@dimen/dimen_big_super</item>
        <item name="android:gravity">end</item>
        <item name="android:textSize">@dimen/text_size_title_small</item>
    </style>

    <style name="profile_all_textview_subtitle">
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="profile_all_textview_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">bottom</item>
        <item name="android:layout_marginStart">@dimen/dimen_big</item>
        <item name="android:layout_marginEnd">@dimen/dimen_big</item>
        <item name="android:layout_marginBottom">@dimen/dimen_medium</item>
    </style>

</resources>